###### Basic app settings
NODE_ENV=development
LISTEN_HOST=localhost
LISTEN_PORT=8080
API_BASE_PATH=api
LOG_LEVEL=warn
# Force application timezone (affects pretty logs)
TZ=UTC
HTTP_REQUEST_BODY_LIMIT_MB=100

###### STT settings
STT_DEFAULT_PROVIDER=assemblyai
STT_ALLOWED_PROVIDERS=
STT_MAX_FILE_SIZE_MB=100
STT_REQUEST_TIMEOUT_SECONDS=15
STT_POLL_INTERVAL_MS=1500
STT_MAX_SYNC_WAIT_MINUTES=3
# Default API key for AssemblyAI (optional; used if request has no apiKey)
ASSEMBLYAI_API_KEY=
